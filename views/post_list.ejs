<html lang="en-gb">
<%- include('_head') -%>
<%- include('_header') -%>
<body>
<h1>Posts List</h1>
<ol>
    <% postList.results.forEach(function(post) {%>
        <li>
            <%= post.message %> - <%= format(post.createdAt, "yyyy-MM-dd HH:mm:ss") %>
        </li>
    <% }); %>
</ol>
</body>
<%- include('_footer') -%>
</html>